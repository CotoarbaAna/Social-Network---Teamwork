<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.paint.LinearGradient?>
<?import javafx.scene.paint.Stop?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="lab6.lab6.UserEventsController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="showEvents" layoutX="250.0" layoutY="50.0" prefHeight="280.0" prefWidth="284.0">
            <columns>
                <TableColumn fx:id="NameCol" prefWidth="144" text="Event" />
                <TableColumn fx:id="DataCol" prefWidth="140" text="Date" />
            </columns>
        </TableView>

        <Button fx:id="AllEvents" layoutX="80.0" layoutY="200.0" mnemonicParsing="false" onAction="#showAllEvents" prefHeight="26.0" prefWidth="100.0" text="All Events" />

        <Button fx:id="Back" layoutX="100.0" layoutY="240.0" mnemonicParsing="false" onAction="#goBack" prefHeight="26.0" prefWidth="60.0" text="Back" />

        <Button fx:id="Leave" layoutX="100.0" layoutY="100.0" mnemonicParsing="false" onAction="#leaveEvent" prefHeight="26.0" prefWidth="60.0" text="Leave" />

        <Text fontSmoothingType="LCD" layoutX="67.0" layoutY="292.0" strokeType="OUTSIDE" strokeWidth="0.0" text="">
            <font>
                <Font size="10.0" />
            </font>
        </Text>
    </children>
</AnchorPane>
