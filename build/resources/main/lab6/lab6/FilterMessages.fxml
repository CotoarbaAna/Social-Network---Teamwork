<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="lab6.lab6.FilterMessagesController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TableView fx:id="showMessages" layoutX="250.0" layoutY="50.0" prefHeight="280.0" prefWidth="284.0">
            <columns>
                <TableColumn fx:id="UserCol" prefWidth="91.199951171875" text="User" />
                <TableColumn fx:id="DataCol" prefWidth="84.00006103515625" text="Date" />
                <TableColumn fx:id="Msg" prefWidth="107.99990844726562" text="Message" />
            </columns>
        </TableView>

        <Button fx:id="SavePDF" layoutX="100.0" layoutY="160.0" mnemonicParsing="false" onAction="#savePdf" prefHeight="26.0" prefWidth="60.0" text="PDF" />

        <Button fx:id="Back" layoutX="100.0" layoutY="200.0" mnemonicParsing="false" onAction="#goBack" prefHeight="26.0" prefWidth="60.0" text="Back" />
    </children>
</AnchorPane>
